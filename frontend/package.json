{
  "name": "youtube-loop-player",
  "version": "1.0.0",
  "type": "module",
  "description": "A YouTube loop player that allows users to paste YouTube URLs and play them with automatic looping",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint src tests --ext .js,.vue",
    "lint:fix": "eslint src tests --ext .js,.vue --fix",
    "format": "prettier --write \"src/**/*.{js,vue,css}\" \"tests/**/*.{js,vue}\""
  },
  "dependencies": {
    "@google/gemini-cli": "^0.10.0",
    "axios": "^1.13.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "keywords": [
    "youtube",
    "loop",
    "player",
    "vue"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^10.5.1",
    "jsdom": "^27.0.1",
    "prettier": "^3.6.2",
    "vite": "^5.4.21",
    "vitest": "^2.1.9"
  }
}
